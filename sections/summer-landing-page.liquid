<section class="banner">
  <div class="banner__media--desktop">
    {% if section.settings.banner-video-desktop != blank %}
      {{ section.settings.banner-video-desktop | video_tag: autoplay: 'true', loop: 'true' }}
    {% else %}
      <img src="{{ section.settings.banner-image-desktop | image_url }}" />
    {% endif %}
  </div>
  <div class="banner__media--mobile">
    {% if section.settings.banner-video-mobile != blank %}
      {{ section.settings.banner-video-mobile | video_tag: autoplay: 'true', loop: 'true' }}
    {% else %}
      <img src="{{ section.settings.banner-image-mobile| image_url }}" />
    {% endif %}
  </div>
  <div class="banner__description">
    <h2>{{ section.settings.banner-heading }}</h2>
    <h3>{{ section.settings.banner-subheading }}</h3>
  </div>
</section>

<section class="section-one">
  {{ section.settings.section-one-image | image_url: width: 1200 | image_tag }}
  <div class="section-one__text">
    {{ section.settings.section-one-text }}
  </div>
  <div id="interview-modal-trigger" class="section-one__play-trigger">
    <img src="{{ 'play-icon.svg' | asset_url }}" />
    <span>Watch the interview</span>
  </div>
  
  <div id="interview-modal" class="interview-modal">
    <div class="interview-modal__content">
      <div class="interview-modal__media-wrapper">
        {{ section.settings.section-one-interview-video-mobile | video_tag: controls: true, class: "interview-video--mobile" }}
        {{ section.settings.section-one-interview-video-desktop | video_tag: controls: true, class: "interview-video--desktop" }}
      <div
      <span id="interview-modal-close" class="interview-modal__close"><img src="{{ 'close-icon-white.svg' | asset_url }}" /></span>
    </div>
  </div>
</section>

<section class="section-two">
  <div class="section-two__media-wrapper">
    {{ section.settings.section-two-video-mobile | video_tag: autoplay: 'true', loop: 'true', class: "section-two__video--mobile" }}
    {{ section.settings.section-two-video-desktop | video_tag: autoplay: 'true', loop: 'true', class: "section-two__video--desktop" }} 
  </div>
</section>

{% for block in section.blocks %}
  {% case block.type %}
    {% when 'block_a' %}
      <section class="block-a {% if block.settings.block-a-right-side-media == true %} row {% else %}row-reverse{% endif %}">
        <div class="block-a__description" style="background-color: {{ block.settings.block-a-text-bg-color }}; color: {{ block.settings.block-a-text-color }}">
          {% if block.settings.block-a-icon != blank  %}
            <div class="block-a__description-icon {{ block.settings.block-a-icon-alignment }}">
              <img src="{{ block.settings.block-a-icon | image_url }}" />
            </div>
          {% endif %}
          <h3>{{ block.settings.block-a-text-title }}</h3>
          <div class="block-a__text">
            {{ block.settings.block-a-text }}
          </div>
        </div>
        <div class="block-a__media">
          {% if block.settings.block-a-video-desktop  %}
          {{ block.settings.block-a-video-desktop | video_tag: autoplay: 'true', loop: 'true', class: 'block-a__video--desktop' }}
          {% else %}
            <img src="{{  block.settings.block-a-image-desktop | image_url }}" class="block-a__image--desktop" />
          {% endif %}
          
          {% if block.settings.block-a-video-mobile  %}
            {{ block.settings.block-a-video-mobile | video_tag: autoplay: 'true', loop: 'true', class: 'block-a__video--mobile' }}
          {% else %}
            <img src="{{ block.settings.block-a-image-mobile | image_url }}" class="block-a__image--mobile" />
          {% endif %}
          {% if block.settings.block-a-tag-label != blank %}
            <a class="tag-link" href="{{ block.settings.block-a-tag-link }}" target="_blank">
            {{ block.settings.block-a-tag-label }}
            </a>
          {% endif %}
        </div>
      </section>
  {% endcase %}
{% endfor %}

<section class="section-seven">
  <div class="section-seven__media">
    <img src="{{ section.settings.section-seven-image-desktop | image_url }}" class="section-seven__image--desktop" />
    <img src="{{ section.settings.section-seven-image-mobile | image_url }}" class="section-seven__image--mobile" />
    {% if section.settings.section-seven-tag-label != blank %}
      <a class="tag-link" href="{{ section.settings.section-seven-tag-link }}">
      {{ section.settings.section-seven-tag-label }}
      </a>
    {% endif %}
  </div>
</section>

<section class="section-eight">
  <div class="section-eight__description">
    <div class="section-eight__text">
      {% if section.settings.section-eight-icon-top != blank  %}<img src="{{ section.settings.section-eight-icon-top | image_url }}" />{% endif %}
        {{ section.settings.section-eight-text }}
      {% if section.settings.section-eight-icon-bottom != blank  %}<img src="{{ section.settings.section-eight-icon-bottom | image_url }}" />{% endif %}
    </div>
  </div>
  <div class="section-eight__media">
    <img src="{{ section.settings.section-eight-image-desktop | image_url }}" class="section-eight__image--desktop" />
    <img src="{{ section.settings.section-eight-image-mobile | image_url }}" class="section-eight__image--mobile" />
    {% if section.settings.section-eight-tag-label != blank %}
      <a class="tag-link" href="{{ section.settings.section-eight-tag-link }}">
      {{ section.settings.section-eight-tag-label }}
      </a>
    {% endif %}
  </div>
</section>

<section class="section-nine">
  <div class="section-nine__media">
    <img src="{{ section.settings.section-nine-image-desktop | image_url }}" class="section-nine__image--desktop" />
    <img src="{{ section.settings.section-nine-image-mobile | image_url }}" class="section-nine__image--mobile" />
    {% if section.settings.section-nine-tag-label != blank %}
      <a class="tag-link" href="{{ section.settings.section-nine-tag-link }}">
      {{ section.settings.section-nine-tag-label }}
      </a>
    {% endif %}
  </div>
</section>

<section class="section-ten">
  <div class="section-ten__media">
    <img src="{{ section.settings.section-ten-image-desktop | image_url }}" class="section-ten__image--desktop" />
    <img src="{{ section.settings.section-ten-image-mobile | image_url }}" class="section-ten__image--mobile" />
    {% if section.settings.section-ten-media-icon  %}
      <img class="section-ten__media-icon" src={{ section.settings.section-ten-media-icon | image_url }} />
    {% endif %}
    <ul class="section-ten__media-text">
      {%  assign section_ten_text_list_arr = section.settings.section-ten-text-list | split: "," %}
      {% for text_item in section_ten_text_list_arr %}
        {% assign text_item_arr = text_item | split: "::"  %}
        <li><span>{{ text_item_arr | first }}</span><span>{{ text_item_arr | last }}</span></li>
      {% endfor %}
    </ul>
  </div>
</section>

