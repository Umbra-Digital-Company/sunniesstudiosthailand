<script>
  $(document).ready(function() {
    const interviewModal = $("#interview-modal")
    const interviewModalTrigger = $("#interview-modal-trigger")
    const interViewModalClose = $("#interview-modal-close")
    const interviewVideoMobile = $(".interview-video--mobile")
    const interviewVideosDesktop = $(".interview-video--desktop")
    
    interviewModalTrigger.click(function(){
      interviewModal.show()     
    })
    interViewModalClose.click(function(){
      interviewVideoMobile.trigger('pause');
      interviewVideosDesktop.trigger('pause');
      interviewModal.hide()     
    })
  });
</script>
