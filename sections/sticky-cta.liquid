<script>
  $("#stick-two-btn").click(function() {
    window.location.href = "{{ section.settings.cta_url }}";
  });
</script>

{% schema %}
  {
    "name": "Sticky CTA",
    "settings": [
      {
        "type": "text",
        "id": "title",
        "label": "Title"
      }, {
        "type": "text",
        "id": "label",
        "label": "Label"
      }, {
        "type": "text",
        "id": "cta_text",
        "label": "CTA Text"
      }, {
        "type": "url",
        "id": "cta_url",
        "label": "CTA Link"
      }
    ],
    "presets": [
      {
        "name": "Sticky CTA"
      }
    ]
  }
{% endschema %}